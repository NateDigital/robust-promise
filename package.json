{
  "name": "robust-promise",
  "version": "1.0.7",
  "description": "Retry promise. Easy to use, retryable promise. Pass it a promise, get a robust-promise!",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/NateDigital/robust-promise.git"
  },
  "keywords": [
    "retry",
    "promise",
    "retryable",
    "backoff",
    "repeat",
    "replay",
    "reliable",
    "robust"
  ],
  "author": "Nathan Pickworth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NateDigital/robust-promise/issues"
  },
  "homepage": "https://github.com/NateDigital/robust-promise#readme",
  "devDependencies": {
    "jest": "^22.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "node"
    ],
    "testRegex": ".*\\.test\\.js$",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "notify": true,
    "reporters": [
      "default"
    ]
  }
}
